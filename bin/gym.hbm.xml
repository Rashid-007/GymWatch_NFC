<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="gym.model.elements.Gym" table="GYM">
		<id name="id" column="GYM_ID" type="java.lang.Integer" />
		<property name="name" column="GYM_Name" type="java.lang.String" />
		<property name="address" column="GYM_Address" type="java.lang.String" />
	</class>
</hibernate-mapping>